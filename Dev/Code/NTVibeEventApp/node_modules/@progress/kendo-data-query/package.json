{
  "name": "@progress/kendo-data-query",
  "description": "Kendo UI data query helpers",
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "version": "1.7.1",
  "main": "dist/npm/main.js",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "es2015": "dist/es2015/main.js",
  "typings": "dist/npm/main.d.ts",
  "jsdelivr": "dist/cdn/main.js",
  "unpkg": "dist/cdn/main.js",
  "scripts": {
    "test": "jest",
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider gulp start",
    "lint": "eslint . --quiet",
    "api-check": "gulp api-check",
    "build-package": "cross-env NODE_OPTIONS=--openssl-legacy-provider gulp build-package build-es2015-bundle",
    "prepare": "husky install"
  },
  "keywords": [
    "Kendo UI",
    "Data Query"
  ],
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.5",
    "@babel/plugin-syntax-typescript": "^7.25.9",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@progress/kendo-typescript-tasks": "dev",
    "@types/jest": "^29.5.14",
    "colors": "^1.1.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^9.0.0",
    "eslint-plugin-jest": "^28.10.0",
    "gulp": "^5.0.0",
    "husky": "^8.0.2",
    "jest": "^29.7.0",
    "jest-preset-typescript": "^1.2.0",
    "semantic-release": "^24.2.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    }
  },
  "jest": {
    "preset": "jest-preset-typescript"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-data-query.git"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular"
  },
  "@progress": {
    "friendlyName": "DataQuery"
  },
  "files": [
    "dist"
  ]
}
