{
  "name": "@progress/kendo-inputs-common",
  "description": "Kendo UI Inputs common package",
  "version": "3.1.2",
  "keywords": [
    "Kendo UI"
  ],
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "dist/npm/main.js",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "es2015": "dist/es2015/main.js",
  "typings": "dist/npm/main.d.ts",
  "jsdelivr": "dist/cdn/main.js",
  "unpkg": "dist/cdn/main.js",
  "sideEffects": false,
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "scripts": {
    "test": "jest",
    "test:ci": "jest",
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider gulp start",
    "lint": "eslint ./src",
    "api-check": "",
    "build-package": "gulp build-ts-package"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "@progress/kendo-drawing": "^1.21.2"
  },
  "devDependencies": {
    "@progress/kendo-drawing": "^1.21.2",
    "@progress/kendo-typescript-tasks": "^11.3.8-dev.20",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "ghooks": "^1.0.3",
    "gulp": "^5.0.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "jest": "^28.1.3",
    "semantic-release": "^24.2.7",
    "ts-jest": "^28.0.7",
    "typescript": "^5.7.3"
  },
  "overrides": {
    "brace-expansion": "2.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "preset": "ts-jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-inputs-common.git"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-inputs-common"
  },
  "release": {
    "branches": [
      {
        "name": "master",
        "channel": "latest"
      },
      {
        "name": "develop",
        "prerelease": true,
        "channel": "dev"
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src/**/*.d.ts"
  ]
}
